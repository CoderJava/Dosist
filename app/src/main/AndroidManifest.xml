<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ysn.dosist">

    <application
        android:name=".views.base.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.ui.activities.splashscreen.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.ui.activities.dashboard.DashboardActivity" />
        <activity android:name=".views.ui.activities.addtransaction.AddTransactionActivity" />
        <activity android:name=".views.ui.activities.welcome.WelcomeActivity" />
    </application>

</manifest>